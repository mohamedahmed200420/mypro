@model TbVote
<form asp-controller="Home" asp-action="vote" method="post" enctype="multipart/form-data">
    <div style="margin:auto" class="form-group" data-select2-id="7">
        <label style="height:40px;width:25% ;font-size:40px">Chose vote</label>
        <select asp-for="VoteId" asp-items="@(new SelectList( ViewBag.Votes,"VoteId","VoteName"))" class="js-example-basic-single select2-hidden-accessible" style="width:100% ;height:50px;font-size:16px;font-weight:700;" data-select2-id="1" tabindex="-1" aria-hidden="true">
        </select>
   
        <input type="submit" value="send" class="btn btn-gradient-primary mb-2" /> 
        @*<span class="dropdown-wrapper" aria-hidden="true"></span></>*@
    </div>

</form>

